{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "mofumofu-desktop",
  "version": "0.1.0",
  "identifier": "com.levish.mofumofu-desktop",
  "build": {
    "beforeDevCommand": "",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "",
    "frontendDist": "http://localhost:5173"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "mofumofu",
        "width": 1200,
        "height": 800,
        "decorations": true,
        "titleBarStyle": "Transparent"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
